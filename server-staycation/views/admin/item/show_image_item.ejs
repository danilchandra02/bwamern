<div class="tab-pane fade active show" id="show-image" role="tabpanel" aria-labelledby="nav-profile-tab">
    <a href="/admin/item" class="btn btn-warning mt-3">Back</a>
    <div class="row">
        <% for(let x=0; x < item.imageId.length; x++) { %>
        <div class="col-4">
            <div class="card shadow mb-4 mt-2">
                <div class="card-body">
                    <img src="http://localhost:3000/images/<%= item.imageId[x].imageUrl %>" alt="image"
                        style="width: 100%; display: block;" />
                </div>
            </div>
        </div>
        <% } %>
    </div>
</div>
